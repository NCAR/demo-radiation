[ccpp-table-properties]
  name = aerosol_model
  type = scheme
  dependencies = musica_aerosol.F90,musica_aerosol_modal.F90

[ccpp-arg-table]
  name  = aerosol_model_init
  type  = scheme
[ model_name ]
  standard_name = aerosol_model_name
  units = 1
  type = character | kind = len=*
  dimensions = ()
  intent = in
[ aerosol ]
  standard_name = aerosol_object
  units = none
  type = aerosol_t | polymorphic = True
  dimensions = ()
  intent = out
[ errcode ]
  standard_name = ccpp_error_code
  units = 1
  type = integer
  dimensions = ()
  intent = out
[ errmsg ]
  standard_name = ccpp_error_message
  units = 1
  type = character | kind = len=512
  dimensions = ()
  intent = out

[ccpp-arg-table]
  name  = aerosol_model_run
  type  = scheme
[ aerosol ]
  standard_name = aerosol_object
  units = none
  type = aerosol_t | polymorphic = True
  dimensions = ()
  intent = inout
[ errcode ]
  standard_name = ccpp_error_code
  units = 1
  type = integer
  dimensions = ()
  intent = out
[ errmsg ]
  standard_name = ccpp_error_message
  units = 1
  type = character | kind = len=512
  dimensions = ()
  intent = out
